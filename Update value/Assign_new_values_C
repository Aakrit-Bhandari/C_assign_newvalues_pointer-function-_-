#include <stdio.h>
#include <string.h>
#include <math.h>
void swap(int *a, int *b)
{
    printf("Enter swapping number:");
    scanf("%d %d", &*a, &*b);
}
int main(void)
{
    int a, b;
    scanf("%d %d", &a, &b);
    printf("Before swapping %d %d\n", a, b);
    swap(&a, &b);
    printf("After swapping %d %d\n", a, b);
}
